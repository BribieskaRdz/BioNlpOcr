<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#626262" />
            <stroke
                android:width="1dp"
                android:color="#626262" />
            <corners
                android:radius="3dp"/>
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#f5f5f5"
                android:endColor="#f5f5f5"
                android:angle="0" />
            <stroke
                android:width="2dp"
                android:color="#626262"/>
            <corners
                android:radius="10dp"/>
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>

